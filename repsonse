    require(['jquery', 'slick'], function($){
        "use strict";

        $(document).ready(function () {
            $('.bh-home-banners-carousel').on('init', function(event, slick){
                adjustCarouselArrows();
            }).slick({
                slidesToShow: 1,
                slidesToScroll: 1,
                dots: false,
                arrows: true,
                infinite: true,
                autoplay: true,
                autoplaySpeed: 3000,
                adaptiveHeight: false
            });

            function adjustCarouselArrows(){
                const $firstBannerMedia = $('.bh-home-banner-media._desktop').first();

                if ($firstBannerMedia.length){
                    const bannerHeight = $firstBannerMedia.outerHeight() / 2;
                    const $carouselArrows = $('.bh-home-banners-carousel .slick-arrow');
                    $carouselArrows.css('top', bannerHeight + 'px');
                }
            }

            adjustCarouselArrows();

            $(window).on('resize', function(){
                adjustCarouselArrows();
            });
        });

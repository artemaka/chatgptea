        require(['jquery'],function($){
            "use strict";
            $(function () {
                
                function getCookie (name) {
                    let value = `; ${document.cookie}`;
                    let parts = value.split(`; ${name}=`);
                    if (parts.length === 2) return parts.pop().split(';').shift();
                }

                let euBarClosed = getCookie('eubarClosed');
                if (!(euBarClosed)) {
                    $('.bh-top_bar-eu').show();
                }

                $(document).on('click', '.bh-top_bar-close', function() {
                    const cookieExpirationDate = new Date();
                    cookieExpirationDate.setMonth(cookieExpirationDate.getMonth() + 3);
                    document.cookie = `eubarClosed=true; path=/; expires=${cookieExpirationDate.toUTCString()}; secure`;
                    $('.bh-top_bar-eu').remove();
                });
            });

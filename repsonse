require(['jquery', 'Magento_Customer/js/customer-data'], function($, customerData) {
    var cart = customerData.get('cart');
    cart.subscribe(function(cartData) {
        if (typeof __adroll !== 'undefined' && cartData.items && cartData.items.length > 0) {
            try {
                __adroll.record_user({"adroll_segments": "3c4c455f"});
                console.log('AdRoll fired - Cart has ' + cartData.items.length + ' items');
            } catch(err) {
                console.error('AdRoll error:', err);
            }
        }
    });
});

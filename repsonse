<script type="ab57153bc2375625897c33a3-text/javascript">
    document.getElementById('toggle-read-more').addEventListener('click', function(event) {
        event.preventDefault(); // Prevent page scroll/jump
        const content = document.querySelector('.read-more-content');
        const link = document.getElementById('toggle-read-more');

        if (content.classList.contains('expanded')) {
            content.classList.remove('expanded');
            link.textContent = 'Read More';
        } else {
            content.classList.add('expanded');
            link.textContent = 'Read Less';
        }
    });
</script>

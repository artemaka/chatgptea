    require(['jquery','mage/translate', 'slick'],function($, $t){
        "use strict";
        $(function () {
            const reviewLimit = 6 * 1.4 * 15 + 1;

            $(window).on("resize", function () {
                collapseReviews();
            }).resize();

            function collapseReviews() {
                let reviews = $('.bh-home-review ._review').length;
                let minHeight = 0;

                $('.bh-home-review ._review').each(function(i) {
                    const review = $(this);
                    const reviewDisplayHeight = review.outerHeight();
                    review.css('height','').addClass('reset');
                    const reviewOriginalHeight = review.outerHeight();
                    review.removeClass('reset');

                    if ( reviewDisplayHeight + 21 < reviewOriginalHeight ) {

                        if ( !(review.siblings('.bh-review-toggle').length) ) {
                            review.attr('data-height', reviewOriginalHeight ).addClass('collapsed').parent().append(`<span class="bh-review-toggle collapsed"><span class="_more">${$t('Show more')}</span><span class="_less">${$t('Show less')}</span>`);
                        }
                    }

                    const contentMinHeight = review.closest('._content').outerHeight();

                    if ( minHeight < contentMinHeight ) {
                        minHeight = contentMinHeight;
                    }

                    if ( i === reviews - 1 && !($('.bh-home-reviews-carousel.slick-initialized').length) ) {
                        $('.bh-home-review ._content').css('min-height', minHeight);
                        reviewsCarousel();
                    }
                });
            }

            $(document).on('click', '.bh-review-toggle', function() {
                const toggle = $(this);
                const review = toggle.siblings('._review');
                const reviewHeight = review.attr('data-height');

                if ( toggle.hasClass('collapsed') ) {
                    review.removeClass('collapsed').animate({ height:reviewHeight },200);
                    toggle.removeClass('collapsed');
                } else {
                    review.animate({ height:reviewLimit },200).addClass('collapsed');
                    toggle.addClass('collapsed');
                }
            });

            function reviewsCarousel() {
                $('.bh-home-reviews-carousel').slick({
                    slidesToShow: 4,
                    slidesToScroll: 4,
                    dots: true,
                    arrows: true,
                    infinite: false,
                    adaptiveHeight: true,
                    responsive: [
                        {
                            breakpoint: 1199,
                            settings: {
                                slidesToShow: 3,
                                slidesToScroll: 3,
                            }
                        },
                        {
                            breakpoint: 1023,
                            settings: {
                                slidesToShow: 2,
                                slidesToScroll: 2,
                            }
                        },
                        {
                            breakpoint: 479,
                            settings: {
                                slidesToShow: 1,
                                slidesToScroll: 1,
                            }
                        }
                    ]
                });
            }
        });
    });</script>   <div class="bh-home-blog"><h2 class="bh-home-title"><a href="https://www.bargainhardware.co.uk/blog/" title="Bargain&#x20;Hardware&#x20;Blog">Bargain Hardware Blog</a></h2><div class="bh-home-blog-content"><ul class="bh-home-posts bh-list_reset">     <li class="bh-home-post _featured" style="background-image: url(https://www.bargainhardware.co.uk/wp/wp-content/uploads/2026/01/refurbished-servers-for-hybrid-cloud-1024x597.webp)"> <div class="_content"><span class="_date">6 January 2026</span> <h3 class="_title">Have you considered refurbished servers for hybrid cloud environments?</h3><p class="_excerpt">Refurbished servers for hybrid cloud environments enable you to achieve enterprise-grade performance without paying enterprise-grade prices. In this article, we consider how refurbished enterprise servers are processed to maintain reliability and performance, and share 5 reasons you should choose refurbished servers for your private cloud. Hybrid cloud is now the default operating model, with 9 in 10 organisations expected to adopt the approach by 2027. Hybrid cloud infrastructure gives you the best of both worlds...</p><a class="_continue" href="https://www.bargainhardware.co.uk/blog/blog/refurbished-servers-for-hybrid-cloud-environments" title="Have you considered refurbished servers for hybrid cloud environments?"><span>Continue&#x20;reading</span><span class="bh-icon-arrow-right"></span></a></div><a class="_link" href="https://www.bargainhardware.co.uk/blog/blog/refurbished-servers-for-hybrid-cloud-environments" title="Have you considered refurbished servers for hybrid cloud environments?"></a></li>     <li class="bh-home-post _regular"><div class="_image">  <div class="_thumb" style="background-image: url(https://www.bargainhardware.co.uk/wp/wp-content/uploads/2025/12/Edge-vs-cloud-when-local-servers-make-sense-vs-cloud-1024x597.webp)"></div></div><div class="_content"><span class="_date">3 December 2025</span> <h3 class="_title">Edge vs cloud